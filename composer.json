{
  "name": "spryker/oauth-agent-connector",
  "description": "OauthAgentConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/agent": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/oauth": "^2.0.0",
    "spryker/oauth-extension": "^1.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/installer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/installer": "If you want to use Installer plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
